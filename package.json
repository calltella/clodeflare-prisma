{
  "name": "vercel-apline",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:next": "next dev",
    "dev:worker": "wrangler dev --local",
    "dev": "concurrently -k -n NEXT,WORKER -c blue,green \"npm run dev:next\" \"npm run dev:worker\"",
    "migrate": "prisma migrate dev",
    "generate": "prisma generate",
    "studio": "prisma studio",
    "build": "opennextjs-cloudflare build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "opennextjs-cloudflare deploy",
    "upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts"
  },
  "dependencies": {
    "@prisma/adapter-d1": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "dotenv": "^17.2.3",
    "hono": "^4.11.7",
    "next": "16.1.4",
    "postcss": "^8.5.6",
    "react": "19.1.4",
    "react-dom": "19.1.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^20.19.30",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "opennextjs-cloudflare": "^1.0.0",
    "prisma": "^7.3.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.7.4",
    "wrangler": "^4.61.0"
  }
}
